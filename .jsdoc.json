{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["models", "lib", "commands", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/category"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "staticFiles": {
                "include": [
                    "./assets"
                ]
            },
            "layoutFile": "./tmpl/layout.tmpl"
        },
        "better-docs": {
            "name": "dota-ihl-bot Documentation",
            "logo": "img/logo.png",
            "navigation": [
                {
                    "label": "Github",
                    "href": "https://github.com/devilesk/dota-ihl-bot"
                }
            ]
        }
    },
    "opts": {
        "destination": "./docs",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "node_modules/better-docs",
        "package": ""
    }
}